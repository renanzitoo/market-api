generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id       String @id @default(uuid())
  name     String
  brand    String
  price    Float
  marketId String
  Market   Market @relation(fields: [marketId], references: [id])
}

model Market {
  id      String    @id @default(uuid())
  name    String
  owner   String
  Product Product[]
}
